{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": ".",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      },
    ],
    "paths": {
      "@ui/*": ["src/ui/*"],
      "@css/*": ["src/css/*"],
      "@page/*": ["src/page/*"],
      "@public/*": ["public/*"],
    }
  },
}
