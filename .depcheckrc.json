{
  "skip-missing": true,
  "ignores": [
    "@typescript-eslint/parser",
    "@astrojs/tailwind",
    "autoprefixer",
    "postcss",
    "tailwindcss",
    "prettier-plugin-tailwindcss",
    "@astrojs/ts-plugin",
    "class-variance-authority",
    "date-fns",
    "puppeteer",
    "jsdom",
    "@astrojs/check",
    "prettier-plugin-astro"
  ],
  "parsers": {
    "**/*.cjs": "es6",
    "**/*.mjs": "es6",
    "**/*.ts": "es6",
    "**/*.tsx": "jsx"
  }
}
